{
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/swisseph.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/swisseph.data",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!(?:swisseph\\.(?:wasm|data)|assets/|favicon\\.ico|placeholder\\.svg|robots\\.txt|.*\\.se1|.*\\.txt)).*)",
      "destination": "/index.html"
    }
  ]
}
