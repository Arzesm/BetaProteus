{
  "rewrites": [
    { "source": "/swisseph.wasm", "destination": "/swisseph.wasm" },
    { "source": "/swisseph.data", "destination": "/swisseph.data" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*\\.wasm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    },
    {
      "source": "/(.*\\.data)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ]
}
