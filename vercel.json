{
  "routes": [
    {
      "src": "/assets/.*\\.wasm$",
      "headers": { "Content-Type": "application/wasm" },
      "continue": true
    },
    {
      "src": "/assets/.*\\.data$",
      "headers": { "Content-Type": "application/octet-stream" },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
